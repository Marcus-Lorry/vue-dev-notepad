{
  "name": "vue-dev-notepad",
  "version": "1.1.0",
  "description": "A floating, draggable, resizable notepad component for Vue.js developers with per-page note persistence",
  "main": "./dist/index.umd.js",
  "module": "./dist/index.es.js",
  "types": "./src/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.es.js",
      "require": "./dist/index.umd.js",
      "types": "./src/index.d.ts"
    },
    "./dist/style.css": "./dist/style.css"
  },
  "files": [
    "dist",
    "src/index.d.ts",
    "README.md"
  ],
  "scripts": {
    "build": "vite build",
    "dev": "vite",
    "preview": "vite preview"
  },
  "keywords": [
    "vue",
    "vue3",
    "notepad",
    "floating",
    "draggable",
    "resizable",
    "notes",
    "developer",
    "development"
  ],
  "author": "Marcus-Lorry",
  "license": "MIT",
  "peerDependencies": {
    "vue": "^3.0.0",
    "vue-router": "^4.0.0"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^4.0.0",
    "typescript": "^5.0.0",
    "vite": "^4.0.0",
    "vue": "^3.0.0",
    "vue-router": "^4.0.0",
    "vue-tsc": "^1.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Marcus-Lorry/vue-dev-notepad.git"
  },
  "bugs": {
    "url": "https://github.com/Marcus-Lorry/vue-dev-notepad/issues"
  },
  "homepage": "https://github.com/Marcus-Lorry/vue-dev-notepad#readme"
}